AEBEM3: AEBEM3.o AEBEM3_T.o GEOM3D.o CGLS.o H3LC.o
	gfortran -o AEBEM3 AEBEM3.o AEBEM3_T.o GEOM3D.o H3LC.o CGLS.o

AEBEM3.o: AEBEM3.FOR
	gfortran -c AEBEM3.FOR

AEBEM3_T.o: AEBEM3_T.FOR
	gfortran -c AEBEM3_T.FOR

GEOM3D.o: ../SHARED/GEOM3D.FOR
	gfortran -c ../SHARED/GEOM3D.FOR

H3LC.o: ../SHARED/H3LC.FOR
	gfortran -c ../SHARED/H3LC.FOR

CGLS.o: ../SHARED/CGLS.FOR
	gfortran -c ../SHARED/CGLS.FOR

clean:
	rm AEBEM3 AEBEM3.OUT AEBEM3.o AEBEM3_T.o GEOM3D.o H3LC.o CGLS.o *~

