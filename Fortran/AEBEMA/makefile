AEBEMA: AEBEMA.o AEBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o
	gfortran -o AEBEMA AEBEMA.o AEBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o

AEBEMA.o: AEBEMA.FOR
	gfortran -c AEBEMA.FOR

AEBEMA_T.o: AEBEMA_T.FOR
	gfortran -c AEBEMA_T.FOR

GEOM2D.o: ../SHARED/GEOM2D.FOR
	gfortran -c ../SHARED/GEOM2D.FOR

GEOM3D.o: ../SHARED/GEOM3D.FOR
	gfortran -c ../SHARED/GEOM3D.FOR

H3ALC.o: ../SHARED/H3ALC.FOR
	gfortran -c ../SHARED/H3ALC.FOR

CGLS.o: ../SHARED/CGLS.FOR
	gfortran -c ../SHARED/CGLS.FOR

clean:
	rm  AEBEMA AEBEMA.OUT AEBEMA.o AEBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o *~
