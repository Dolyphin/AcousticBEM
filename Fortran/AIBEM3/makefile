AIBEM3: AIBEM3.o AIBEM3_T.o GEOM3D.o CGLS.o H3LC.o
	gfortran -o AIBEM3 AIBEM3.o AIBEM3_T.o GEOM3D.o H3LC.o CGLS.o

AIBEM3.o: AIBEM3.FOR
	gfortran -c AIBEM3.FOR

AIBEM3_T.o: AIBEM3_T.FOR
	gfortran -c AIBEM3_T.FOR

GEOM3D.o: ../SHARED/GEOM3D.FOR
	gfortran -c ../SHARED/GEOM3D.FOR

H3LC.o: ../SHARED/H3LC.FOR
	gfortran -c ../SHARED/H3LC.FOR

CGLS.o: ../SHARED/CGLS.FOR
	gfortran -c ../SHARED/CGLS.FOR

clean:
	rm AIBEM3.o AIBEM3_T.o GEOM3D.o H3LC.o CGLS.o AIBEM3 AIBEM3.OUT *~

