AIBEMA: AIBEMA.o AIBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o
	gfortran -o AIBEMA AIBEMA.o AIBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o

AIBEMA.o: AIBEMA.FOR
	gfortran -c AIBEMA.FOR

AIBEMA_T.o: AIBEMA_T.FOR
	gfortran -c AIBEMA_T.FOR

GEOM2D.o: ../SHARED/GEOM2D.FOR
	gfortran -c ../SHARED/GEOM2D.FOR

GEOM3D.o: ../SHARED/GEOM3D.FOR
	gfortran -c ../SHARED/GEOM3D.FOR

H3ALC.o: ../SHARED/H3ALC.FOR
	gfortran -c ../SHARED/H3ALC.FOR

CGLS.o: ../SHARED/CGLS.FOR
	gfortran -c ../SHARED/CGLS.FOR

clean:
	rm AIBEMA.o AIBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o AIBEMA AIBEMA.OUT
