AIBEMA: AIBEMA.o AIBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o
	gfortran -o AIBEMA AIBEMA.o AIBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o

AIBEMA.o: AIBEMA.FOR
	gfortran -c AIBEMA.FOR

AIBEMA_T.o: AIBEMA_T.FOR
	gfortran -c AIBEMA_T.FOR

GEOM2D.o: GEOM2D.FOR
	gfortran -c GEOM2D.FOR

GEOM3D.o: GEOM3D.FOR
	gfortran -c GEOM3D.FOR

H3ALC.o: H3ALC.FOR
	gfortran -c H3ALC.FOR

CGLS.o: CGLS.FOR
	gfortran -c CGLS.FOR

clean:
	rm AIBEMA.o AIBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o AIBEMA
