AMBEMA: AMBEMA.o AMBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o INTEIG.o CGEIG.o
	gfortran -o AMBEMA AMBEMA.o AMBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o INTEIG.o CGEIG.o

AMBEMA.o: AMBEMA.FOR
	gfortran -c AMBEMA.FOR

AMBEMA_T.o: AMBEMA_T.FOR
	gfortran -c AMBEMA_T.FOR

GEOM2D.o: ../SHARED/GEOM2D.FOR
	gfortran -c ../SHARED/GEOM2D.FOR

GEOM3D.o: ../SHARED/GEOM3D.FOR
	gfortran -c ../SHARED/GEOM3D.FOR

H3ALC.o: ../SHARED/H3ALC.FOR
	gfortran -c ../SHARED/H3ALC.FOR

CGLS.o: ../SHARED/CGLS.FOR
	gfortran -c ../SHARED/CGLS.FOR

INTEIG.o: ../SHARED/INTEIG.FOR
	gfortran -c ../SHARED/INTEIG.FOR

CGEIG.o: ../SHARED/CGEIG.FOR
	gfortran -c ../SHARED/CGEIG.FOR

clean:
	rm AMBEMA AMBEMA.OUT AMBEMA.o AMBEMA_T.o GEOM2D.o GEOM3D.o H3ALC.o CGLS.o INTEIG.o CGEIG.o *~
